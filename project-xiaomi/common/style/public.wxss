
/* 文字单行省略号 */
.font-hidden {
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}

/* 文字多行省略号 */
.font-multicell-hidden {
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

/* 修改默认按钮文案样式 */
button {
  font-weight: 500 !important;
}
  